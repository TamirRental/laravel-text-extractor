{
    "name": "tamir/document-extraction",
    "description": "A Laravel package for extracting structured data from documents via OCR APIs. Ships with Koncile AI provider.",
    "keywords": [
        "tamir",
        "laravel",
        "document-extraction",
        "ocr",
        "koncile"
    ],
    "homepage": "https://github.com/tamir/laravel-document-extraction",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Tamir",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^11.0||^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Tamir\\DocumentExtraction\\": "src/",
            "Tamir\\DocumentExtraction\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tamir\\DocumentExtraction\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tamir\\DocumentExtraction\\DocumentExtractionServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
